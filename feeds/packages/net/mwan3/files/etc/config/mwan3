
config globals 'globals'
	option mmx_mask '0x3F00'
	option rtmon_interval '5'

config interface 'wan1'
	option initial_state 'offline'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '60'
	option timeout '5'
	option interval '10'
	option failure_interval '3'
	option recovery_interval '5'
	option down '5'
	option up '5'
	option track_method 'ping'
	list track_ip 'www.google.com'
	list track_ip 'www.facebook.com'
	list track_ip '1.1.1.1'
	list track_ip '9.9.9.9'
	list track_ip 'ipv6.google.com'
	option family 'ipv4'
	option enabled '0'

